pipeline {
   ageny any {
   stages {
      stage('clone') {
         checkout scm
      stage('bulid') {
         steps {
             bat "mvn clean"
            }
        }
        stage('test') {
            steps {
                  bat "mvn test"
               }
         }
         stage('deploy') {
            steps {
                  bat "mvn package"
               }
            }
       }
}
